    "def Venue_max(m):   # Function for venues with corersponding number of wins for team who bats second\n",
    "    dict5={item[0]:0 for item in m} #Creates a dictionary with venues and values 0 \n",
    "    for row in m:\n",
    "        if not(row[6]==\"\" or row[6]==\"super over\"):  # Checking of win by wickets ,i.e. team batting second wins and increment count for the venue\n",
    "            dict5[row[0]] +=1\n",
    "    return(dict5)\n",
    "\n",
    "def Total_venue(m):  #Function for venues with total matches played in them \n",
    "    dict6={item[0]:0 for item in m}\n",
    "    for row in m:\n",
    "        dict6[row[0]] +=1\n",
    "    return(dict6)\n",
    "\n",
    "def best_chasing_venue(m):\n",
    "    win_percent_dict = {key: Venue_max(m)[key] / Total_venue(m)[key] for key in Venue_max(m) }  # Dictionary of Winning ratio of venues of team batting second\n",
    "    max_pair = max(win_percent_dict.items(), key=lambda item: item[1])  # Finding pair with max ratio\n",
    "    return(max_pair)\n",
    "    "
